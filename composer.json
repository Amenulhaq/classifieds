{
    "name": "laraclass/classifieds",
    "description": "Classifieds package for laraclass.",
    "keywords": ["lavalite", "Classifieds"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Laraclass\\Classifieds\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraclass\\Classifieds\\Providers\\ClassifiedsServiceProvider"
            ],
            "aliases": {
                "Classifieds"       : "Laraclass\\Classifieds\\Facades\\Classifieds"
            }
        }
    },
    "minimum-stability": "dev"
}
